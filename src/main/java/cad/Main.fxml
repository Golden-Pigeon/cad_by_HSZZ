               <MenuItem mnemonicParsing="false" onAction="#onListRecordMenuItemAction" text="List Record" />
         </items>
       </Menu>
         <Menu mnemonicParsing="false" text="Tools">
           <items>
               <CheckMenuItem fx:id="painterToolBarMenuItem" mnemonicParsing="false" onAction="#onPainterToolMenuItemAction" selected="true" text="Painter Tool" />
               <CheckMenuItem fx:id="colorToolBarMenuItem" mnemonicParsing="false" onAction="#onColorToolMenuItemAction" selected="true" text="Color Tool" />
           </items>
         </Menu>
         <Menu mnemonicParsing="false" text="Window">

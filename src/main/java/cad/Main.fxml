         </TilePane>
         <Label prefHeight="20.0" prefWidth="75.0" text="   Painter" textAlignment="CENTER" />
      </children>
   </VBox>
   </left>
   <right>
   <VBox fx:id="colorToolBar" alignment="CENTER" prefHeight="150.0" prefWidth="78.0" style="-fx-background-color: #aaaaaa; -fx-background-radius: 5;" BorderPane.alignment="CENTER">
      <children>
         <TilePane hgap="5.0" prefColumns="2" prefHeight="145.0" prefRows="4" prefWidth="37.0" vgap="5.0">
